						"type": {
							"coding": [
								{
									"system": "http://bpd.bcbs.com/CodeSystem/BCBSIdentifierTypeCS",
									"code": "PCRN",
									"display": "Provider Cross Reference Number"
								}
							]
						},
						"value": "462791677",
						"period": {
							"end": "2023-05-18"
						}
					},



if(org.getOldMstrProvId()!= null) {
        Identifier CCNIdentifier = new Identifier();
        Coding ccnidCoding = new Coding();
        ccnidCoding.setSystem(Constants.CCNID_SYS);
        ccnidCoding.setCode(Constants.PCRN);
        ccnidCoding.setDisplay(Constants.PROV_CROSS_REF_NO);
        CCNIdentifier.setType(new CodeableConcept().addCoding(ccnidCoding));
        CCNIdentifier.setValue(org.getOldMstrProvId());
        identifiers.add(CCNIdentifier);
        if(org.getOldTerminationDate() != null) {
        	Identifier dd = new Identifier();
            DateTimeFormatter inputFormatter = DateTimeFormatter.ofPattern(Constants.DATE_FORMAT);
    		LocalDate date = LocalDate.parse(org.getOldTerminationDate(),inputFormatter);
    		DateTimeFormatter outputFormatter = DateTimeFormatter.ofPattern(Constants.DATE_FORMAT_yyyy_MM_dd);
    		String outPutDate = date.format(outputFormatter);
            Period period = new Period();
    		period.setEndElement(new DateTimeType(outPutDate));
            org.setOldTerminationDate(outPutDate);
            }
        }
